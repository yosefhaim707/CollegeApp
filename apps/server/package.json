    "sync:seed": "ts-node-dev src/jobs/sync.ts"
  },
  "dependencies": {
    "@f1-stats-suite/types": "workspace:*",
    "argon2": "0.31.2",
    "axios": "1.6.7",
    "bottleneck": "2.19.5",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.18.3",
    "express-rate-limit": "7.1.5",
    "helmet": "7.0.0",
    "http-errors": "2.0.0",
    "ioredis": "5.4.1",
    "jsonwebtoken": "9.0.2",
    "mongoose": "7.6.3",
    "morgan": "1.10.0",
    "node-cron": "3.0.3",
    "pino": "8.17.0",
    "pino-http": "8.5.0",
    "ws": "8.16.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "9.0.4",
    "@types/morgan": "1.9.9",
    "@types/node": "20.11.25",
    "@types/ws": "8.5.10",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "jest": "29.7.0",
    "supertest": "6.4.2",
    "ts-jest": "29.1.1",
    "ts-node-dev": "2.0.0",
    "tsup": "7.2.0",
    "typescript": "5.4.2"
  }
}
